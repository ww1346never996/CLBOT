<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.clbot.mapper.QuestionMapper">

    <resultMap id="BaseResultMap" type="com.example.clbot.pojo.Question">
        <id column="id" property="id" />
        <result column="group_id" property="group_id" />
        <result column="question" property="question" />
        <result column="answer" property="answer" />
        <result column="last_edit_user" property="last_edit_user" />
    </resultMap>

    <select id="Base_Column_List">
        id, group_id, question, answer, last_edit_user
    </select>

</mapper>